{
  "rules": {
    "camelcase": 0,
    "quotes": [2, "single", "avoid-escape"],
    "space-before-function-paren": 1,
    "space-in-parens": 2,
    "object-curly-spacing": 2,
    "array-bracket-spacing": 2,
    "computed-property-spacing": 2,
    "space-before-blocks": 2,
    "keyword-spacing": 2,
    "no-lonely-if": 2,
    "comma-style": 2,
    "no-underscore-dangle": 0,
    "no-constant-condition": 0,
    "no-multi-spaces": 0,
    "strict": 0,
    "key-spacing": 0,
    "no-shadow": 0,
    "indent": ["error", 4, {
        "MemberExpression": "off"
    }]
  },
  "globals": {
    "d3": true,
    "module": true
  },
  "plugins": [
    "html"
  ],
  "settings": {
    "html/report-bad-indent": 2,
    "html/indent": "space"
  },
  "env": {
    "browser": true,
    "amd": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
}
